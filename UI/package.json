    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue-cli-plugin-quasar": "3.x",
    "vue-template-compiler": "2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
